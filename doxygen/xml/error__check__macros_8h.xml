        <name>CHECK_MALLOC_ERROR</name>
        <param><defname>type</defname></param>
        <param><defname>x</defname></param>
        <initializer>    do {                                       \
    type * ptr = (type*)(x);                                                   \
    if(ptr == NULL) {                                                          \
        fprintf(stderr, &quot;Malloc Error for type %s on Line %d in %s&quot;,           \
                #type, __LINE__, __FILE__);                                    \
        perror(&quot; &quot;);                                                           \
        free(ptr);                                                             \
        ptr = NULL;                                                            \
        goto malloc_error;                                                     \
    }                                                                          \
} while(0)</initializer>

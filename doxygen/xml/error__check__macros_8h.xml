      <memberdef kind="define" id="error__check__macros_8h_1a3d05d4ff53571d917db5a64a5afe5ac5" prot="public" static="no">
        <name>CHECK_DPGLUE_ERROR</name>
        <param><defname>x</defname></param>
        <param><defname>M</defname></param>
        <initializer>    do {                                          \
    int retval = (x);                                                          \
    switch(retval) {                                                           \
    case 0:                                                                    \
        break;                                                                 \
    case -1:                                                                   \
        fprintf(stderr, &quot;DP_Glue Error: %d (%s) on Line %d in %s\n&quot;,           \
                retval, M, __LINE__, __FILE__);                                \
        goto error;                                                            \
    default:                                                                   \
        fprintf(stderr, &quot;DP_Glue Error: %d (%s) on Line %d in %s\n&quot;,           \
                retval, M, __LINE__, __FILE__);                                \
        goto error;                                                            \
    }                                                                          \
} while(0)</initializer>
